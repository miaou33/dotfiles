{

  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
	"black": "#0d0c0c",
  	"blue": "#8ba4b0",
  	"cyan": "#8ea4a2",
  	"green": "#8a9a7b",
  	"magenta": "#a292a3",
  	"red": "#c4746e",
  	"white": "#FEF5ED", //"#C8C093"
  	"yellow": "#c4b28a",
	"darkgrey": "#575656",
  	"bright_black": "#a6a69c",
  	"bright_blue": "#7FB4CA",
  	"bright_cyan": "#7AA89F",
  	"bright_green": "#87a987",
  	"bright_magenta": "#938AA9",
  	"bright_red": "#E46876",
  	"bright_white": "#c5c9c5",
  	"bright_yellow": "#E6C384"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:magenta",
          "foreground": "p:black",
          "leading_diamond": "\ue0b2",
          "properties": {
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\ue62a "
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}  ",
          "trailing_diamond": "<transparent,p:magenta>\ue0b2</>",
          "type": "os"
        },
        {
          "background": "p:green",
          "foreground": "p:black",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "  \uf489  {{ .Name }}  ",
          "trailing_diamond": "<transparent,p:green>\ue0b2</>",
          "type": "shell"
        },
        {
          "background": "p:yellow",
          "foreground": "p:black",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "  MEM: {{ round .PhysicalPercentUsed .Precision }}% | {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB  ",
          "trailing_diamond": "<transparent,p:yellow>\ue0b2</>",
          "type": "sysinfo"
        },
        {
          "background": "p:darkgrey",
          "foreground": "#d6deeb",
          "leading_diamond": "\ue0b2",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:bright_magenta",
          "foreground": "p:black",
          "leading_diamond": "\ue0b2",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": " \u2665 {{ .CurrentDate | date .Format }} |",
          "type": "time"
        },
        {
          "style": "plain",
          "template": " \uf292 ",
          "type": "root"
        },
        {
          "properties": {
			"style": "full",
            //"folder_icon": "\uf07b ",
            "folder_separator_icon": "  \uf061  ",
            "home_icon": "\ueb06 "
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u2570\u2500 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:white",
    "template": "\ue285 "
  },
  "version": 2
}
